<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:textAlignment="gravity"
    tools:context=".HomeActivity"
    tools:ignore="MissingClass">

    <Button
        android:id="@+id/button3"
        android:layout_width="270dp"
        android:layout_height="76dp"
        android:layout_marginBottom="16dp"
        android:hapticFeedbackEnabled="false"
        android:text="@string/your_heart_rate"
        android:textAlignment="viewStart"
        android:textAllCaps="false"
        android:textSize="18sp"
        app:backgroundTint="#9A969D"
        app:iconTint="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/button7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.411"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button6"
        android:layout_width="270dp"
        android:layout_height="70dp"
        android:layout_marginBottom="8dp"
        android:hapticFeedbackEnabled="false"
        android:text="@string/your_messages"
        android:textAlignment="viewStart"
        android:textAllCaps="false"
        android:textSize="18sp"
        app:backgroundTint="#9A969D"
        app:iconTint="#FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/button5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.411"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button4"
        android:layout_width="127dp"
        android:layout_height="36dp"
        android:layout_marginBottom="40dp"
        android:text="@string/logout"
        android:textAllCaps="false"
        android:textColor="#FFFFFF"
        app:backgroundTint="#9A969D"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:rippleColor="#9A969D"
        android:layout_gravity="center_horizontal"
        android:gravity="center_horizontal"/>

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="222dp"
        android:layout_height="185dp"
        android:contentDescription="@string/todo"
        app:layout_constraintBottom_toTopOf="@+id/button3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.707"
        app:srcCompat="@drawable/profilepicsample" />

    <Button
        android:id="@+id/button7"
        android:layout_width="180dp"
        android:layout_height="37dp"
        android:layout_marginBottom="28dp"
        android:text="@string/recordnewevents"
        android:textAllCaps="false"
        android:textSize="12sp"
        app:backgroundTint="#9A969D"
        app:layout_constraintBottom_toTopOf="@+id/button6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.268"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_gravity="center_horizontal"
        android:gravity="center_horizontal"/>

    <Button
        android:id="@+id/button5"
        android:layout_width="180dp"
        android:layout_height="37dp"
        android:layout_marginBottom="28dp"
        android:backgroundTint="#9A969D"
        android:text="@string/recordnewmessages"
        android:textSize="12sp"
        app:backgroundTint="#9A969D"
        app:layout_constraintBottom_toTopOf="@+id/button4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.268"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_gravity="center_horizontal"
        android:gravity="center_horizontal"/>

</androidx.constraintlayout.widget.ConstraintLayout>